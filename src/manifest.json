{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_extName__",
    "description": "__MSG_extDescription__",
    "version": "1.3.0",
    "permissions": [
        "alarms",
        "scripting"
    ],
    "host_permissions": [
        "https://meet.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "context/meet-google-com.js"
            ],
            "css": [
                "context/meet-google-com.css"
            ]
        }
    ],
    "icons": {
        "48": "img/icon.48.png",
        "128": "img/icon.128.png"
    }
}